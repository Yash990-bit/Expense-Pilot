{
    "name": "expense-tracker-fullstack",
    "version": "1.0.0",
    "description": "Full-stack expense tracker application",
    "scripts": {
        "install-backend": "cd backend && npm install",
        "install-frontend": "cd frontend/expense-tracker && npm install",
        "install-all": "npm run install-backend && npm run install-frontend",
        "dev-backend": "cd backend && npm run dev",
        "dev-frontend": "cd frontend/expense-tracker && npm run dev",
        "build": "cd frontend/expense-tracker && npm install && npm run build",
        "vercel-build": "cd frontend/expense-tracker && npm install && npm run build"
    },
    "keywords": [
        "expense-tracker",
        "fullstack",
        "react",
        "express"
    ],
    "author": "",
    "license": "ISC",
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "bcryptjs": "^3.0.3",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.2.1",
        "jsonwebtoken": "^9.0.3",
        "mongoose": "^9.1.2",
        "multer": "^2.0.2",
        "xlsx": "^0.18.5"
    }
}